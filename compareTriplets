def compareTriplets(a, b):
    alice_score = 0
    bob_score = 0
    n = 0
    for n in range(0, 3):
        if a[n] > b[n]:
            alice_score += 1
        elif a[n] < b[n]:
            bob_score += 1
        else:
            alice_score = alice_score
            bob_score = bob_score
        
    score = [alice_score, bob_score]        
    return score
